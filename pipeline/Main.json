							"name": "processor_azure_pipe",
							"type": "ExecutePipeline",
							"dependsOn": [
								{
									"activity": "processor_hcp_pipe",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "processor_azure_pipe",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true
