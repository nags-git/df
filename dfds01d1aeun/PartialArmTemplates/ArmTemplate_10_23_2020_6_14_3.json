{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfds01d1aeun"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/az_sql_ls')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"parameters": {
					"sql_account_url": {
						"type": "string",
						"defaultValue": "ssds01d1aeun.database.windows.net"
					},
					"sql_database_name": {
						"type": "string",
						"defaultValue": "sdbds01d1a-inventory"
					},
					"sql_username": {
						"type": "string",
						"defaultValue": "sqlsa"
					},
					"sql_password": {
						"type": "string",
						"defaultValue": "az9h%%eKxS%LxECcYJ%Aw4lDol%8DUEN_0BW0vfqpITkq2qvQzZ8U3D_nrizLUQT"
					}
				},
				"annotations": [],
				"type": "AzureSqlDatabase",
				"typeProperties": {
					"connectionString": {
						"type": "SecureString",
						"value": "**********"
					}
				},
				"connectVia": {
					"referenceName": "hcp-Ir",
					"type": "IntegrationRuntimeReference"
				}
			},
			"dependsOn": []
		}
	]
}