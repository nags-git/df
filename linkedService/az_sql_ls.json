{
	"name": "az_sql_ls",
	"type": "Microsoft.DataFactory/factories/linkedservices",
	"properties": {
		"parameters": {
			"sql_account_url": {
				"type": "String",
				"defaultValue": "qa-capitatfl-sql-server.database.windows.net"
			},
			"sql_database_name": {
				"type": "String",
				"defaultValue": "qa-capitatfl-db-hcpdata"
			},
			"sql_username": {
				"type": "String",
				"defaultValue": "test"
			},
			"sql_password": {
				"type": "String",
				"defaultValue": "Capita@123456789"
			}
		},
		"annotations": [],
		"type": "AzureSqlDatabase",
		"typeProperties": {
			"connectionString": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=@{linkedService().sql_account_url};Initial Catalog=@{linkedService().sql_database_name};User ID=@{linkedService().sql_username};Password=@{linkedService().sql_password}"
		}
	}
}