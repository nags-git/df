{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dev-capita-df"
		},
		"az_cosmos_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'az_cosmos_ls'"
		},
		"az_func_update_objectprops_functionKey": {
			"type": "secureString",
			"metadata": "Secure string for 'functionKey' of 'az_func_update_objectprops'"
		},
		"az_sql_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'az_sql_ls'"
		},
		"az_strg_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'az_strg_ls'"
		},
		"hcp_http_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ns01.tenant01.coe-hcp01.hitachivantara.com/rest/"
		},
		"http_rest_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://tenant01.coe-hcp01.hitachivantara.com/query"
		},
		"on_query_csv_upload_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/2a3390b4-6a54-48b5-812a-35320c2fab9c/resourceGroups/dev-capita-rg/providers/Microsoft.Storage/storageAccounts/devcapitaevidentialst"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"az_cosmos_ls_connectionString": {
						"value": "[parameters('az_cosmos_ls_connectionString')]"
					},
					"az_func_update_objectprops_functionKey": {
						"value": "[parameters('az_func_update_objectprops_functionKey')]"
					},
					"az_sql_ls_connectionString": {
						"value": "[parameters('az_sql_ls_connectionString')]"
					},
					"az_strg_ls_connectionString": {
						"value": "[parameters('az_strg_ls_connectionString')]"
					},
					"hcp_http_ls_properties_typeProperties_url": {
						"value": "[parameters('hcp_http_ls_properties_typeProperties_url')]"
					},
					"http_rest_ls_properties_typeProperties_url": {
						"value": "[parameters('http_rest_ls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"on_query_csv_upload_properties_typeProperties_scope": {
						"value": "[parameters('on_query_csv_upload_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}